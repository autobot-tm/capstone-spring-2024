{
  "name": "web-user",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "commit": "cz",
    "start-dev": "env-cmd -f ./env/.env.dev react-scripts start",
    "start-prod": "env-cmd -f ./env/.env.prod react-scripts start",
    "build-dev": "env-cmd -f ./env/.env.dev react-scripts build",
    "build-prod": "env-cmd -f ./env/.env.prod react-scripts build",
    "deploy-dev": "CI=false yarn build-dev && serverless deploy --stage dev",
    "deploy-prod": "CI=false yarn build-prod && serverless deploy --stage prod",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|md|vue)\"",
    "prepare": "husky install",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "postinstall": "yarn prepare",
    "reset": "rimraf node_modules; yarn"
  },
  "dependencies": {
    "@ant-design/icons": "5.2.6",
    "@react-oauth/google": "0.12.1",
    "@reduxjs/toolkit": "1.9.7",
    "@sentry/react": "7.94.1",
    "antd": "5.11.0",
    "axios": "1.6.0",
    "buffer": "6.0.3",
    "env-cmd": "10.1.0",
    "i18next": "23.6.0",
    "i18next-browser-languagedetector": "7.1.0",
    "moment": "2.30.1",
    "postcss": "8.4.31",
    "postinstall": "0.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-i18next": "13.3.1",
    "react-redux": "8.1.3",
    "react-router-dom": "6.18.0",
    "react-scripts": "5.0.1",
    "rimraf": "5.0.5",
    "sass": "1.69.5",
    "web-vitals": "2.1.4"
  },
  "devDependencies": {
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.53.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-unused-imports": "3.0.0",
    "husky": "8.0.3",
    "node-sass": "9.0.0",
    "prettier": "3.0.3",
    "serverless-certificate-creator": "1.6.0",
    "serverless-cloudfront-invalidate": "1.12.2",
    "serverless-s3-sync": "3.2.0",
    "serverless-website-domain": "3.0.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "packageManager": "yarn@3.5.0"
}
