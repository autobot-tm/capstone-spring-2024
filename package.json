{
  "name": "web-user",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "commit": "git-cz",
    "start-dev": "env-cmd -f ./env/.env.dev react-scripts start",
    "build-dev": "env-cmd -f ./env/.env.dev react-scripts build",
    "start-prod": "env-cmd -f ./env/.env.prod react-scripts start",
    "build-prod": "env-cmd -f ./env/.env.prod react-scripts build",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|md|vue)\"",
    "prepare": "husky install",
    "postinstall": "yarn prepare"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@reduxjs/toolkit": "^1.9.7",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "antd": "^5.11.0",
    "axios": "^1.6.0",
    "buffer": "^6.0.3",
    "env-cmd": "^10.1.0",
    "postcss": "^8.4.31",
    "postinstall": "^0.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.18.0",
    "react-scripts": "5.0.1",
    "sass": "^1.69.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.3",
    "prettier": "^3.0.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "packageManager": "yarn@3.5.0"
}
